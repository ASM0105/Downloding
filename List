    initialize: function (adUnitKeys) {
        for (var i = 0; i < adUnitKeys.length; i++) {
            a9slots.push({
                "slotID": pm.dfp.units[adUnitKeys[i]].id,
                "slotName": pm.dfp.units[adUnitKeys[i]].dfpId,

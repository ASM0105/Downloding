        },
    refreshVisibleAdsSpecial: function(ids) {
        var units = [];
        for(var i=0;i<ids.length;i++) {
            units.push(pm.dfp.units[ids[i]]);
            googletag.display(ids[i])
        }
        googletag.pubads().refresh(units);
    },
    refreshVisibleAds: function() {
        if (pm.dfp.rendered) {
            var ids = pm.prebid.getVisibleAdUnitIds();
            pm.dfp.refreshVisibleAdsSpecial(ids);
        } else {
            window.setTimeout(function() { pm.dfp.refreshVisibleAds(); }, 1000);
        }
    },
    initialize: function () {
        googletag.cmd.push(function() {
            var topLeaderboardMobile = googletag.sizeMapping().addSize([0, 0], [320, 50]).addSize([320, 700], []).addSize([1050, 200], []).build();
            var midRectangleMobile = googletag.sizeMapping().addSize([0, 0], [300, 250]).addSize([320, 700], []).addSize([1050, 200], []).build();
            var topLeaderboard = googletag.sizeMapping().addSize([0, 0], [320, 50]).addSize([320, 700], [728, 90]).addSize([1050, 200], [728, 90]).build();
            var topPushdown = googletag.sizeMapping().addSize([0, 0], []).addSize([320, 700], [1, 1]).addSize([1050, 200], [1, 1]).build();
            var leaderboardRectangle = googletag.sizeMapping().addSize([0, 0], [300, 250]).addSize([320, 700], [[728, 90], [300, 250]]).addSize([1050, 200], [[728, 90], [300, 250]]).build();
            var desktopRectangle = googletag.sizeMapping().addSize([0, 0], []).addSize([320, 700], [[300, 250], [300, 600]]).addSize([1050, 200], [[300, 250], [300, 600]]).build();
            var radioHalfpage = googletag.sizeMapping().addSize([0, 0], [320, 50]).addSize([320, 700], [300, 600]).addSize([1050, 200], [300, 600]).build();
            var radioSkyscraper = googletag.sizeMapping().addSize([0, 0], []).addSize([320, 700], [160, 600]).addSize([1050, 200], [160, 600]).build();
            var adUnitKeys = pm.prebid.getVisibleAdUnitIds();
            pm.a9.initialize(adUnitKeys); /* set up the A9 array of units first */
            for(var i=0;i<adUnitKeys.length;i++) {
                var mapping = null;
                switch (pm.dfp.units[adUnitKeys[i]].mapping) {
                    case "topLeaderboardMobile":
                        mapping = topLeaderboardMobile;
                        break;
                    case "midRectangleMobile":
                        mapping = midRectangleMobile;
                        break;
                    case "topLeaderboard":
                        mapping = topLeaderboard;
                        break;
                    case "topPushdown":
                        mapping = topPushdown;
                        break;
                    case "leaderboardRectangle":
                        mapping = leaderboardRectangle;
                        break;
                    case "desktopRectangle":
                        mapping = desktopRectangle;
                        break;
                    case "radioHalfpage":
                        mapping = radioHalfpage;
                        break;
                    case "radioSkyscraper":
                        mapping = radioSkyscraper;
                        break;
                }
                pm.dfp.units[adUnitKeys[i]] = googletag.defineSlot(pm.dfp.units[adUnitKeys[i]].dfpId, pm.dfp.units[adUnitKeys[i]].sizes, pm.dfp.units[adUnitKeys[i]].id).setTargeting("screen_location", pm.dfp.units[adUnitKeys[i]].screenLocation).defineSizeMapping(mapping).addService(googletag.pubads());
            }
            //googletag.pubads().enableSingleRequest();
            googletag.pubads().collapseEmptyDivs();
            googletag.pubads().disableInitialLoad();
            googletag.enableServices();
            pm.dfp.rendered = true;
        });
    }
};
//--><!]]></script>

            "bottom-leaderboard-rectangle":{"code":"bottom-leaderboard-rectangle","sizes":[300,250],"bids":[{"bidder":"indexExchange","params":{"id":38,"siteID":199943}},{"bidder":"appnexus","params":{"placementId":11770893}},{"bidder":"sovrn","params":{"tagid":471943}},{"bidder":"conversant","params":{"secure":113512,"site_id":"113512"}},{"bidder":"aol","params":{"network":"11189.1","placement":"4648037"}}]}
            }
    },
    getVisibleAdUnitIds: function() {
        var ids = [],
            requestingDesktop = pm.prebid.browserWidth > 740,
            units = requestingDesktop ? pm.prebid.units.desktop : pm.prebid.units.phone;
        if (requestingDesktop && !pm.prebid.units.desktop["top-pushdown"]) {
            ids.push("top-pushdown"); /* since no headers bid on this unit it's not in the bids array, we have to manually stick it into the IDs array */
        }
        for (key in units) {
            if (document.getElementById(key)) {
                ids.push(key);
            }
        }
        return ids;
    },
    getAdUnits: function(ids) {
        var units = [],
            requestingDesktop = pm.prebid.browserWidth > 740,
            unitReference = requestingDesktop ? pm.prebid.units.desktop : pm.prebid.units.phone;
        for (var i=0;i<ids.length;i++) {
            if (ids[i] !== "top-pushdown") {
                units.push(unitReference[ids[i]]);
            }
        }
        return units;
    },
    bidsPassbackHandler: function() {
        pbjs.setTargetingForGPTAsync();
        googletag.pubads().setTargeting("url", window.location.pathname);
        googletag.pubads().setTargeting("mediatype", window.page.mediaType);
        googletag.pubads().setTargeting("articletype", window.page.articleType);
        googletag.pubads().setTargeting("development", false);
        pm.dfp.refreshVisibleAds();
    },
    sendAdserverRequest: function() {
        if (pm.prebid.adserverRequestSent) return;
        pm.prebid.adserverRequestSent = true;
        googletag.cmd.push(function() {
            pbjs.que.push(pm.prebid.bidsPassbackHandler);
        });
    },
    render: function () {
        var ids = pm.prebid.getVisibleAdUnitIds(),
            units = pm.prebid.getAdUnits(ids);
        pbjs.que.push(function() {
            pbjs.bidderSettings = {
                standard: {
                    adserverTargeting: [
                        {
                            key: "hb_bidder",
                            val: function(bidResponse) {
                                return bidResponse.bidderCode;
                            }
                        }, {
                            key: "hb_adid",
                            val: function(bidResponse) {
                                return bidResponse.adId;
                            }
                        }, {
                            key: "hb_pb",
                            val: function(bidResponse) {
                                var cpm = bidResponse.cpm;
                                if (cpm < 50.00) {
                                    return (Math.floor(cpm * 100) / 100).toFixed(2);
                                }
                                else {
                                    return "50.00";
                                }
                            }
                        }
                    ]
                }
            };
            pbjs.setBidderSequence('random');
            pbjs.addAdUnits(units);
            pbjs.enableAnalytics([{
                provider: 'pubwise',
                options: {
                    site: "c46d5d04-242c-43c8-b621-fe62698427a4",
                    endpoint: "https://api.pubwise.io/api/v4/event/add/"
                }
            }]);
            pbjs.requestBids({
                bidsBackHandler: pm.prebid.sendAdserverRequest
            });
        });
    },
    refreshBidsSpecial: function(ids) {
        var units = pm.prebid.getAdUnits(ids);
        pbjs.que.push(function() {
            pbjs.requestBids({
                timeout: pm.prebid.timeout,
                adUnitCodes: ids,
                bidsBackHandler: function() {
                    pbjs.setTargetingForGPTAsync(ids);
                    /*googletag.pubads().setTargeting("url", window.location.pathname);
                    googletag.pubads().setTargeting("mediatype", window.page.mediaType);
                    googletag.pubads().setTargeting("articletype", window.page.mediaType);*/
                    pm.dfp.refreshVisibleAdsSpecial(ids);
                }
            });
        });
    },
    refreshBids: function() {
        var ids = pm.prebid.getVisibleAdUnitIds();
        pm.prebid.refreshBidsSpecial(ids);
    }
};

//--><!]]></script>
